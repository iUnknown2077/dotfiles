{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "display": {
    "separator": " | ",
    "color": {
      "output": "white"
    }
  },
  "logo": {
    "type": "small",
    "padding": {
      "right": 1
    },
    "color": {
      "1": "cyan",
      "2": "white"
    }
  },
  "modules": [
    {
      "type": "title",
      "key": " ",
      "keyColor": "cyan",
      "color": {
        "user": "white",
        "at": "cyan",
        "host": "white"
      }
    },
    "break",
    {
      "type": "os",
      "key": " ",
      "keyColor": "yellow",
      "format": "{pretty-name}"
    },
    {
      "type": "kernel",
      "key": " ",
      "keyColor": "yellow",
      "format": "{1} {2}"
    },
    {
      "type": "uptime",
      "key": " 󰅐",
      "keyColor": "yellow"
    },
    {
      "type": "command",
      "key": " 󰔠",
      "keyColor": "yellow",
      "text": "echo $(~/.config/fastfetch/progress.sh)",
      "shell": "/usr/bin/bash"
    },
    "break",
    {
      "type": "host",
      "key": " 󰾰",
      "keyColor": "blue"
    },
    {
      "type": "cpu",
      "key": " 󰻠",
      "keyColor": "blue"
    },
    {
      "type": "gpu",
      "key": " 󰍛",
      "keyColor": "blue"
    },
    {
      "type": "memory",
      "key": " ",
      "keyColor": "blue"
    },
    {
      "type": "display",
      "key": " 󰍹",
      "keyColor": "blue"
    },
    "break",
    /*{
      "type": "de",
      "key": " 󰕮",
      "keyColor": "green"
    },*/
    {
      "type": "wm",
      "key": " ",
      "keyColor": "green"
    },
    {
      "type": "shell",
      "key": " ",
      "keyColor": "green"
    },
    {
      "type": "terminal",
      "key": " ",
      "keyColor": "green"
    },
    {
      "type": "packages",
      "key": " 󰏖",
      "keyColor": "green"
    },
    "break",
    {
      "type": "colors",
      "paddingLeft": 2,
      "symbol": "circle"
    }
  ]
}
